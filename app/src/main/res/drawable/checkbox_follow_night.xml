<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:width="12dp" android:height="12dp" android:drawable="@mipmap/icon_circle_select_night" android:gravity="center" />
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:width="12dp" android:height="12dp" android:drawable="@mipmap/icon_circle_unselect_night" android:gravity="center" />
        </layer-list>
    </item>
</selector>