<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <!-- 圆角度 -->
            <corners android:radius="14dp" />
            <!-- 使用填充色 -->
            <solid android:color="?attr/general_background_color" />
            <!-- 边框 -->
            <stroke
                android:width="1dp"
                android:color="?attr/color_list_icon_bg_stroke" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape>
            <!-- 圆角度 -->
            <corners android:radius="12dp" />
            <!-- 使用填充色 -->
            <solid android:color="?attr/color_grid_icon_bg" />
        </shape>
    </item>
</layer-list>